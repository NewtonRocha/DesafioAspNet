<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@ViewData["Title"]</title>

    <!-- Bootstrap core CSS -->

    <link href="lib/gentelella/production/css/bootstrap.min.css" rel="stylesheet">

    <link href="lib/gentelella/production/fonts/css/font-awesome.min.css" rel="stylesheet">
    <link href="lib/gentelella/production/css/animate.min.css" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="lib/gentelella/production/css/custom.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="lib/gentelella/production/css/maps/jquery-jvectormap-2.0.3.css" />
    <link href="lib/gentelella/production/css/icheck/flat/green.css" rel="stylesheet" />
    <link href="lib/gentelella/production/css/floatexamples.css" rel="stylesheet" type="text/css" />

    <script src="lib/gentelella/production/js/jquery.min.js"></script>
    <script src="lib/gentelella/production/js/nprogress.js"></script>

    <!--[if lt IE 9]>
          <script src="lib/gentelella/production/../assets/js/ie8-responsive-file-warning.js"></script>
          <![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
            <script src="lib/gentelella/production/https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
            <script src="lib/gentelella/production/https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
          <![endif]-->

</head>


<body class="nav-md">

    <div class="container body">


        <div class="main_container">

            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">

                    <div class="navbar nav_title" style="border: 0;">
                        <a href="lib/gentelella/production/index.html" class="site_title"><i class="fa fa-user"></i> <span>Portal do Usuário</span></a>
                    </div>
                    <div class="clearfix"></div>

                    <!-- menu prile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img src="lib/gentelella/production/images/img.jpg" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Bem vindo,</span>
                            <h2>Test User</h2>
                        </div>
                    </div>
                    <!-- /menu prile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">

                        <div class="menu_section">
                            <h3>Menu</h3>
                            <ul class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-line-chart"></i> Dashboard <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="lib/gentelella/production/index.html">Dashboard</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/index2.html">Dashboard2</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/index3.html">Dashboard3</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-users"></i> Usuários <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="lib/gentelella/production/form.html">Usuários</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/form_advanced.html">Atividades</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/form_validation.html">Cargos</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/form_wizards.html">Perfis</a>
                                        </li>
                                        <li>
                                            <a href="lib/gentelella/production/form_upload.html">Clientes</a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">

                <div class="nav_menu">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                    </nav>
                </div>

            </div>
            <!-- /top navigation -->
            <!-- page content -->
            @RenderBody()        
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Teste técnico feito por Newton Rocha
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>

    <script src="lib/gentelella/production/js/bootstrap.min.js"></script>

    <!-- gauge js -->
    <script type="text/javascript" src="lib/gentelella/production/js/gauge/gauge.min.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/gauge/gauge_demo.js"></script>
    <!-- bootstrap progress js -->
    <script src="lib/gentelella/production/js/progressbar/bootstrap-progressbar.min.js"></script>
    <!-- icheck -->
    <script src="lib/gentelella/production/js/icheck/icheck.min.js"></script>
    <!-- daterangepicker -->
    <script type="text/javascript" src="lib/gentelella/production/js/moment/moment.min.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/datepicker/daterangepicker.js"></script>
    <!-- chart js -->
    <script src="lib/gentelella/production/js/chartjs/chart.min.js"></script>

    <script src="lib/gentelella/production/js/custom.js"></script>

    <!-- flot js -->
    <!--[if lte IE 8]><script type="text/javascript" src="lib/gentelella/production/js/excanvas.min.js"></script><![endif]-->
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.pie.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.orderBars.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.time.min.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/date.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.spline.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.stack.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/curvedLines.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/flot/jquery.flot.resize.js"></script>
    

    <!-- worldmap -->
    <script type="text/javascript" src="lib/gentelella/production/js/maps/jquery-jvectormap-2.0.3.min.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/maps/gdp-data.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/maps/jquery-jvectormap-world-mill-en.js"></script>
    <script type="text/javascript" src="lib/gentelella/production/js/maps/jquery-jvectormap-us-aea-en.js"></script>
    <!-- pace -->
    <script src="lib/gentelella/production/js/pace/pace.min.js"></script>
    <!-- skycons -->
    <script src="lib/gentelella/production/js/skycons/skycons.min.js"></script>

    @RenderSection("scripts", required: false)

<!-- /footer content -->
</body>

</html>
